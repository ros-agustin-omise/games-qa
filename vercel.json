{
  "version": 2,
  "name": "playverse",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/games/(.*)",
      "dest": "/games/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}